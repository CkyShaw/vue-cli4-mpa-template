{
	"name": "vue-cli4-mpa-template",
	"version": "0.2.0",
	"private": true,
	"scripts": {
		"dev": "vue-cli-service serve --module",
		"dev:all": "vue-cli-service serve --module index,module1,module2",
		"build": "vue-cli-service build --module",
		"build:all": "vue-cli-service build --module index,module1,module2",
		"lint": "vue-cli-service lint",
		"prettier": "prettier --write \"src/**/*.{js,jsx,ts,json,vue,css,less,scss,md,html}\""
	},
	"dependencies": {
		"ant-design-vue": "^1.7.2",
		"axios": "^0.21.1",
		"core-js": "^3.8.3",
		"echarts": "^5.0.1",
		"highcharts": "^8.2.2",
		"moment": "^2.29.1",
		"mqtt": "^4.2.6",
		"resize-observer-polyfill": "^1.5.1",
		"throttle-debounce": "^3.0.1",
		"vue": "^2.6.11",
		"vue-router": "^3.5.1",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.11",
		"@vue/cli-plugin-eslint": "~4.5.11",
		"@vue/cli-plugin-router": "~4.5.11",
		"@vue/cli-plugin-vuex": "~4.5.11",
		"@vue/cli-service": "~4.5.11",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-component": "^1.1.1",
		"babel-plugin-import": "^1.13.3",
		"compression-webpack-plugin": "^7.1.2",
		"eslint": "^7.18.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-vue": "^7.5.0",
		"lint-staged": "^10.5.3",
		"postcss-pxtorem": "^5.1.1",
		"prettier": "^2.2.1",
		"stylus": "^0.54.8",
		"stylus-loader": "^3.0.2",
		"vue-cli-plugin-webpack-bundle-analyzer": "^2.0.0",
		"vue-template-compiler": "^2.6.11"
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8",
		"chrome >= 38"
	],
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{json,css,less,scss,md,html}": [
			"prettier --write",
			"git add"
		],
		"*.{js,jsx,ts,vue}": [
			"vue-cli-service lint",
			"git add"
		]
	}
}
